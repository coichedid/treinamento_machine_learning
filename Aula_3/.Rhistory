dexptrunc <- function(x,rate=1,a=0,b=Inf){
n   <- length(x)
onx <- as.numeric(x<a | x>b)
aux <- pexp(b,rate)-pexp(a,rate)
out <- onx*rep(0,n)+(1-onx)*dexp(x,rate)/aux
return(out)
}
